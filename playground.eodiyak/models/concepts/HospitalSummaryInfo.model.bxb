structure (HospitalSummaryInfo) {
  description (병원 요약 정보)
  property (hpId) {
    type (HpID)
    min (Required) max (One)
  }
  property (startTime) {
    type (StartTime)
    min (Required) max (One)
  }
  property (endTime) {
    type (EndTime)
    min (Required) max (One)
  }
  property(isPharmacy){
    type(Pharmacy)
    min (Optional) max (One)
  }
}
