structure (HospitalInfo) {
	description (병원의 정보들)
	property (name) {
		type (DutyName)
		min (Required) max (One)
	}
	property (addr) {
		type (DutyAddr)
		min (Optional) max (One)
	}
	property (dutytel) {
		type (DutyTel1)
		min (Optional) max (One)
	}
	property (endtime) {			// 병원 마감시간
		type (EndTime)
		min (Optional) max (One)
	}

}
