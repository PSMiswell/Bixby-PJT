action (SelectHospital) {
  description (유저가 병원을 선택하도록 한다)
  type(Constructor)
  collect {
    computed-input (hospital) {
      type (NearHospitalList)
      min (Required) max (One)
      compute{
        intent{
          goal: NearHospitalList
          route: GetHospitalList
        }
      }
    }
    input(selectNum){
      type(entity.Ordinal)
      min (Optional) max (One)
    }
  }
  output (HospitalSummaryInfo)
}
