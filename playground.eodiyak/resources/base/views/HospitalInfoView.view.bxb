result-view {
  match : HospitalInfo (hosInfo)
    /// concepts의 HospitalInfo에서 정의된 것 매치
  
  message {
    if ("exists(hosInfo.dgidIdName)") {
      template ("선택하신 병원의 정보입니다.")
    } else {
      template ("선택하신 약국의 정보입니다.")
    }
  }

  render {
    layout {
      layout-macro (InfoDetail) {
        param (detail) {
          expression (hosInfo)
        }
      }
    }
	}
}