input-view {
	match : DgHospitalInfoList (this)
	message {template ("현재 진료중인 병원입니다.")}
	render {
		selection-of (this) {
			where-each (item) {
				layout-macro (DgList){
					param (resultsum) {
						expression(item)
					}
				}
			}
		}
	}
	conversation-drivers {
		conversation-driver {
			template ("가까운 병원 알려줘")
		}
		conversation-driver {
			template ("가까운 약국 알려줘")
		}
	}
	
}