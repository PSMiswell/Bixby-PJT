input-view {
	match : NearPharmacyList (this)
	message {template ("가까운 약국입니다.")}
	render {
		selection-of (this) {
			where-each (item) {
				layout-macro (NearPharmListLayout){
					param (pharms) {
						expression(item)
					}
				}
			}
		}
	}
	conversation-drivers {
		conversation-driver {
			template ("가까운 병원 알려줘")
		}
		conversation-driver {
			template ("가까운 내과 알려줘")
		}
	}
}